proc LIB_GE_get_parent_proc {{type ""}} {
  if {[info level] <= 2} {
    return "!!LEVEL 0!!"
  } else {
    if {$type == "all"} {
      return [info level -2]
    } else {
      return [lindex [info level -2] 0]
    }
  }
}
