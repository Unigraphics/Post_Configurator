  #regsub -all -- \\\] $output \\\\\] output
  #regsub -all -- \\\} $output \\\\\} output
  #regsub -all -- \\\{ $output \\\\\{ output
  #regsub -all -- \\\" $output \\\\\" output
  #regsub -all -- \\\$ $output \\\\\$ output
  #return $output

  #use faster code
  regsub -all {[][*+?{}()<>|.^$\\]} $output {\\&} output
