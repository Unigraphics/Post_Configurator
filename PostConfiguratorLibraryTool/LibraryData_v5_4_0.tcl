proc LIB_FH_get_pathnames {{dir .}} {

  if {[file isdirectory $dir] && ![catch {
    set dir [file normalize $dir]
    set dir [glob -nocomplain -directory $dir -type d *]
  } err]} {
    return $dir
  } else {
