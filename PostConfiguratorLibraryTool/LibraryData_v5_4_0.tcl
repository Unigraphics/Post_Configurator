    "+ZYX" -
    "ZYX" {
      #ZYX
      #    ClC2           C1S2S3-S1C3     S1S3+C1S2C3
      #    SlC2           S1S2S3+C1C3     S1S2C3-C1S3
      #    -S2            C2S3            C2C3
      set sin_b [expr (-1*$m2)]
      set cos_b [expr (1*cos(asin(-1*$m2)))]
      if {[EQ_is_zero $cos_b]} {
        if {$coord_rot_address == "first"} {
          set sin_a [expr -1*$m3]
          set cos_a $m4
          set cos_b 0.0
          set sin_c 0.0
          set cos_c 1.0
