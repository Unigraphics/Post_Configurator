proc LIB_GE_create_json_array {arg} {
  regsub -all "," $arg "" arg
  if {[llength $arg] > 1} {
    set list ""
    for { set n 0 } { $n < [llength $arg] } { incr n } {
      if {$n < [expr [llength $arg]-1]} {
        append list "\"[string trim [lindex $arg $n]]\", "
      } else {
        append list "\"[string trim [lindex $arg $n]]\""
      }
    }
    return $list
  } else {
    return "\"$arg\""
  }
}
