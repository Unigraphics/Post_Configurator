          MOM_do_template delay
        }
      } @OUTPUT
      LIB_GE_command_buffer_output
    }
  } else {
    LIB_GE_command_buffer MOM_delay_message
    LIB_GE_command_buffer {
      if {$mom_delay_mode == "SECONDS" || [LIB_GE_catch_do_template delay_rev]} {
        MOM_do_template delay BUFFER
