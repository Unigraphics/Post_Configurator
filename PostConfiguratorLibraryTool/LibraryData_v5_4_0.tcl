              set decompose [CONF_CTRL_moves decompose_first_move_std_xul]
              LIB_SPF_decompose_block_template $decompose [CONF_CTRL_moves $mom_motion_event] FORCE [join $decompose]
            } @DECOMPOSEXUL
            }

          1 {
