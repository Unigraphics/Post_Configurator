    LIB_do_template INITIAL
  }
  array set init_tool_axis "0 $mom_tool_axis(0) 1 $mom_tool_axis(1) 2 $mom_tool_axis(2)"
  LIB_write_pretreatment "mom_out_angle_pos init_tool_axis"

