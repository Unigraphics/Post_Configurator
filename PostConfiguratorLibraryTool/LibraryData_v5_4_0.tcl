proc MOM_start_of_program_include {} {
  global lib_spf

  #<MAN check to avoid wrong output #1506>
  set full_circle 0
  if {[llength [info command LIB_PT_get_header_var]]} {
    if {[LIB_PT_get_header_var mom_kin_arc_output_mode] == "FULL_CIRCLE"} {
      set full_circle 1
    }
