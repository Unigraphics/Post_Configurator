          set sin_a 0.0
          set cos_a 1.0
          set cos_b 0.0
          set sin_c [expr (-1*$m7)]
          set cos_c $m4
        }
      } else {
        set cos_a [expr $m0/$cos_b]
        set sin_a [expr (1*$m1)/$cos_b]
        set cos_c [expr $m8/$cos_b]
        set sin_c [expr (1*$m5)/$cos_b]
      }
      set tmpval [expr (($cos_a*$sin_b*$sin_c) - ($cos_c*$sin_a)) - $m3]
