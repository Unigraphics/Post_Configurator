      if {[info exists prev_convert_mom_out_angle_pos]} {
        set mom_pos(3) $prev_convert_mom_out_angle_pos(0)
      } else {
        set mom_pos(3) 0
      }

      MOM_reload_variable -a mom_pos

    }
    "sim_to_pos" {
      # motion SIM to POS
    }
  }
