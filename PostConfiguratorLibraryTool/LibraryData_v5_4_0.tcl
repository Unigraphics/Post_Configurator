  set w1(0) [expr {$u1(1) * $v1(2) - $u1(2) * $v1(1)}]
  set w1(1) [expr {$u1(2) * $v1(0) - $u1(0) * $v1(2)}]
  set w1(2) [expr {$u1(0) * $v1(1) - $u1(1) * $v1(0)}]
