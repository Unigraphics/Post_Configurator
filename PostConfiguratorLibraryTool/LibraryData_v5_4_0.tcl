  set m1 $rotation_matrix(1)
  set m2 $rotation_matrix(2)
  set m3 $rotation_matrix(3)
  set m4 $rotation_matrix(4)
  set m5 $rotation_matrix(5)
  set m6 $rotation_matrix(6)
  set m7 $rotation_matrix(7)
  set m8 $rotation_matrix(8)
