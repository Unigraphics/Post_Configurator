proc LIB_GE_sort_value {valuelist {sequence 0}} {
  if {$sequence == 1} {
    set valuelist [lsort -decreasing -real $valuelist]
  } elseif {$sequence == 0} {
    set valuelist [lsort -increasing -real $valuelist]
  }
  return [lindex $valuelist 0]

}
