proc LIB_Shell_environment_handling {} {

  global lib_ge_env lib_ge_slash lib_ge_installed_machines

  set lib_ge_env(post_pool_dir)     [LIB_Shell_format_path_names [MOM_ask_env_var "UGII_POST_POOL_DIR_NG"]]
