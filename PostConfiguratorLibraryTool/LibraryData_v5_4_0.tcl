      } else {

        MOM_do_template [CONF_CTRL_moves linear_template]
      }
    } @MOVE
    LIB_GE_command_buffer_output
  }

  #---------------------------------------------------------------------------------
  LIB_GE_command_buffer MOM_linear_move_LIB_ENTRY_end
