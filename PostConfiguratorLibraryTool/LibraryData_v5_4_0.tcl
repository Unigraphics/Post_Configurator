#____________________________________________________________________________________________
proc LIB_GE_get_proc {{type ""}} {
  if {[info level] < 2} {
    return "!!LEVEL 0!!"
  } else {
    if {$type == "all"} {
      return [info level -1]
    } else {
      return [lindex [info level -1] 0]
    }
